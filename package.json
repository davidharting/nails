{
  "name": "nails",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "run-p build:web build:workers",
    "build:web": "next build",
    "build:workers": "yarn babel tasks -x .ts --out-dir .dist --delete-dir-on-start --source-maps",
    "dev": "run-p dev:next dev:redis",
    "dev:next": "next dev",
    "dev:redis": "docker run -p 6379:6379 redis",
    "dev:workers": "",
    "prisma:client:gen": "prisma generate",
    "e2e:dev": "cypress open",
    "e2e:run": "cypress run",
    "format:check": "prettier --check .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "prisma:migrate:gen": "prisma migrate save --name init --experimental",
    "prisma:migrate:up": "prisma migrate up --experimental",
    "prisma:refresh": "npm run prisma:migrate:gen && npm run prisma:migrate:up && npm run prisma:client:gen",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci",
    "wait": "wait-on http://localhost:3000"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.2",
    "bootstrap": "^4.4.1",
    "bull": "^3.13.0",
    "next": "9.3.4",
    "react": "16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@prisma/cli": "^2.0.0-beta.2",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/bull": "^3.12.2",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/testing-library__jest-dom": "^5.0.3",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-root-import": "^6.5.0",
    "cypress": "^4.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.2"
  }
}
